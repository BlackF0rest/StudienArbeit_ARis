@article{linAugmentedRealitySmart2025,
  title = {Augmented {{Reality Smart Glasses}}: {{Current Challenges}} and {{Future Innovations}} in {{Wearable Technology}}},
  shorttitle = {Augmented {{Reality Smart Glasses}}},
  author = {Lin, Weiye},
  year = {2025},
  month = jan,
  journal = {Theoretical and Natural Science},
  volume = {83},
  pages = {209--215},
  doi = {10.54254/2753-8818/2025.20151},
  abstract = {Wearable technology has become one of the most significant research areas today, offering both portability and solutions to various human needs. However, current smart glasses face challenges in hardware capabilities and user interaction. Despite their potential, smart glasses have not been embraced as widely as smartwatches and smartphones. Furthermore, existing smart glasses do not offer significantly greater functions to justify their higher prices compared to smartphones. Moreover, user feedback for smart glasses products like HoloLens indicates that these devices suffer from non-intuitive operation, inconvenience and limited features. Most of the current literature focuses on the usage of smart glasses in specific scenarios, but there is limited in-depth analysis of the challenges in the interaction layer of smart glasses. This article aims to address these challenges by examining the deficiencies in the hardware and interaction layers of smart glasses, using Google Glass and Microsoft HoloLens 2 as case studies. By analyzing existing literature and practical applications, this study explores potential future development directions to improve usability, enhance user experience, and expand market adoption of AR smart glasses.}
}

